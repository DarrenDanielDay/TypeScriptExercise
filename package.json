{
  "name": "ts-playground",
  "version": "1.0.0",
  "description": "Typescript playground",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project ./tsconfig.production.json",
    "clean": "ts-node ./scripts/clean.ts",
    "format": "prettier --write .",
    "gene:test-files": "ts-node ./scripts/gene-test-files.ts",
    "lint": "prettier --check .",
    "start": "ts-node -p . --files",
    "start:prod": "node ./dist/main.js",
    "test": "ts-node -O \"{ \\\"noUnusedLocals\\\":false, \\\"noUnusedParameters\\\":false }\" ./tests/main.ts",
    "test:types": "yarn test types",
    "test:runtime": "yarn test utils",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-decorators": "^0.20.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
